  },
  "double for": {
    "body": [
      "for (int i = 0; i < ${1:limit}; i++) {",
      "\tfor (int j = 0; j < ${2:limit}; j++) {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "prefix": "2for"
  },
  "for": {
    "body": [
      "for (${1:size_t} ${2:i} = ${3:0}; $2 < ${4:count}; $2${5:++}) {",
      "\t$0",
      "}"
    ],
    "prefix": "for"
  },
  "function": {
    "body": ["${2:void} ${1:fun}(${3:void}) {", "\t$0", "}"],
    "prefix": "fn"
  },
  "printf": {
    "body": ["printf(\"${1:%s}\\n\"$2);"],
    "prefix": "pf"
  }
